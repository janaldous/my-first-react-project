  "jest": {
    "setupFiles": [
      "<rootDir>/test-shim.js",
      "<rootDir>/test-setup.js"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": "<rootDir>/test-preprocessor.js"
    },
    "testMatch": [
      "**/src/test/**/*.(ts|tsx|js)"
    ],
    "moduleNameMapper": {
      "\\.(css|scss)$": "<rootDir>/src/mocks/styleMock.js"
    }
